[
    {
        "settings": [ "master" ],

        "name": "tabview",

        "commonlib": "./test-lib.js",

        "config": {
            "comboUrl": "http://mojito-int.trunk.development.manhattan.bf1.yahoo.com",
            "baseUrl":"http://user-jiliang.trunk.development.manhattan.bf1.yahoo.com"
        },

        "startProxyServer": true,
        "proxyConfig": "./proxy_config.json",

        "dataprovider": {

            "shelf": {
                "group": "client",
                "params": {
                    "test": "testshelf.js",
                    "page": "$$config.baseUrl$$/"
                }
            },
            "bbcnews": {
                "group": "client",
                "params": {
                    "test": "testbbcnews.js",
                    "page": "$$config.baseUrl$$/read.html?id=bbcnews"
                }
            },
            "shelf_withrouter": {
                "group": "router",
                "params": {
                    "test": "testshelf.js",
                    "page": "$$config.comboUrl$$/"
                }
            },
            "bbcnews_withrouter": {
                "group": "router",
                "params": {
                    "test": "testbbcnews.js",
                    "page": "$$config.comboUrl$$/read.html?id=bbcnews"
                }
            },

            "login" : {
                "group" : "func",
                "params" :{
                    "scenario": [
                        {
                            "page": "http://news.yahoo.com"
                        },
                        {
                            "controller": "locator",
                            "params": {
                                "value": "#submit",
                                "click": true
                            }
                        },
                        {
                            "page": "http://finance.yahoo.com"
                        },
                        {
                            "test": "test-title.js",
                            "title": "Yahoo! Finance - Business Finance, Stock Market, Quotes, News"
                        }
                    ]
                }
            }
        }

    },

    {
        "settings": [ "environment:development" ]
    }

]
